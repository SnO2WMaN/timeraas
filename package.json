{
  "name": "countdown-as-a-service",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "run-s -sln gqlgen i18ngen",
    "build": "next build",
    "start": "next start",
    "gqlgen": "gql-gen",
    "i18ngen": "typesafe-i18n --no-watch",
    "storybook:dev": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook -s ./public",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "run-s -cn lint:*",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:prettier": "prettier --check **/*.{json,yml,graphql}",
    "fmt": "run-s -cn fmt:*",
    "fmt:eslint": "yarn lint:eslint --fix",
    "fmt:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@prisma/client": "^3.0.2",
    "apollo-server-micro": "^3.3.0",
    "clsx": "^1.1.1",
    "graphql": "^15.5.3",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.2",
    "react-use": "^17.3.1",
    "recoil": "^0.4.1",
    "styled-components": "^5.3.1",
    "typesafe-i18n": "^2.37.4"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@babel/core": "^7.15.5",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^2.1.3",
    "@graphql-codegen/typescript-react-apollo": "^3.1.3",
    "@graphql-codegen/typescript-resolvers": "^2.2.1",
    "@shopify/eslint-plugin": "^40.4.0",
    "@shopify/prettier-config": "^1.1.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/react": "^17.0.20",
    "@types/styled-components": "^5.1.14",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "^27.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "2.4.0",
    "prisma": "^3.0.2",
    "storybook-addon-next-router": "^3.0.8",
    "tailwindcss": "^2.2.14",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "type-fest": "^2.2.0",
    "typescript": "^4.4.2"
  }
